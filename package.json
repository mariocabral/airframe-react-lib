{
  "name": "airframe-react-lib",
  "version": "1.0.0",
  "description": "Based on https://github.com/0wczar/airframe-react project",
  "author": "mariocabral",
  "license": "MIT",
  "repository": "mariocabral/airframe-react-lib",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "bhelp": "microbundle-crl build --help",
    "build": "microbundle-crl --no-compress --format modern,es,cjs,umd && yarn dist-styles",
    "dist-styles": "cp -r ./src/styles ./dist/styles",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.10",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.4",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "rollup-plugin-collect-sass": "^1.0.9"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "@owczar/dashboard-style--airframe": "^0.1.23",
    "bootstrap": "^4.5.2",
    "react-grid-layout": "^1.1.1",
    "react-helmet": "^6.1.0",
    "react-responsive": "^8.1.0",
    "react-router-dom": "^5.2.0",
    "reactstrap": "^8.6.0"
  }
}
